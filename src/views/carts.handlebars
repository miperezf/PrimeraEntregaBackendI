<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{{title}}</title>
  </head>
  <body>
    <h1>{{title}}</h1>
    <ul>
      {{#each carts}}
        <li>
          <strong>Carrito ID:</strong>
          {{_id}}
          <ul>
            {{#each products}}
              <li>
                <strong>Producto ID:</strong>
                {{productId}}
                <strong>Cantidad:</strong>
                {{quantity}}
              </li>
            {{/each}}
          </ul>
        </li>
      {{/each}}
    </ul>
  </body>
</html>