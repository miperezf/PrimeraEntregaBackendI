<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Productos</title>
  </head>
  <body>
    <h1>Lista de Productos</h1>
    <ul>
      {{#each products}}
        <li>
          <h2>{{title}}</h2>
          <p>{{description}}</p>
          <p>Price: ${{price}}</p>
          <p>Category: {{category}}</p>
          <p>Stock: {{stock}}</p>
        </li>
      {{/each}}
    </ul>
  </body>
</html>